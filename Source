--[[
  |Version = 2.0 (Big Update)
   |Scripts : Infinity Yield, Fisch, BedWars, King Legacy, Rival, Prison Life, The Strongest Battle Ground, Randomly Generated Droids, Parkour -- A total of 9 scripts
    |Lastest Update = 2025-02-06
     |Made By @Ledolium 
]]

--- RayField
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
--- RayField

--- Window Start
local Window = Rayfield:CreateWindow({
   Name = "Script Hub",
   Icon = 10783216485, -- Icon in Topbar. Can use Lucide Icons (string) or Roblox Image (number). 0 to use no icon (default).
   LoadingTitle = "L's Script Hub",
   LoadingSubtitle = "by @Ledolium V2.0",
   Theme = "DarkBlue", -- Check https://docs.sirius.menu/rayfield/configuration/themes

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, -- Prevents Rayfield from warning when the script has a version mismatch with the interface

   --- ConfigurationSaving
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "LSHub", -- Create a custom folder for your hub/game
      FileName = "SaveFile"
   },
   --- ConfigurationSaving

   --- Discord Start
   Discord = {
      Enabled = false, -- Prompt the user to join your Discord server if their executor supports it
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   --- Discord End

   --- Key System Start
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "@Ledolium's Script Hub",
      Subtitle = "Key System",
      Note = "My Name", -- Use this to tell the user how to get a key
      FileName = "Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"Sentiard"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})
--- Key System End

--- Notify System
Rayfield:Notify({
   Title = "Load",
   Content  = "Loading Complete!",
   Duration = 6.5,
   Image = 9425193028,
})

--- Credit Tab
local CreditTab = Window:CreateTab("Credit", 903318290) -- Title, Image
local CreditSection = CreditTab:CreateSection("Credit")

--- Credittab TextBox
local Label = CreditTab:CreateLabel("Credit : @Ledolium", 8120666025, Color3.fromRGB(15, 20, 30), true) -- Title, Icon, Color, IgnoreTheme

--- Admin Scripts Tab
local AdminTab = Window:CreateTab("Admin Scripts", 924121207) -- Title, Image
local AdminSection = AdminTab:CreateSection("Admin Scripts")

--- Infinity Yield Script
local Button = AdminTab:CreateButton({
   Name = "Infinity Yield",
   Callback = function()
   loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
   end,
})

--- QuirkyCMD Script
local Button = AdminTab:CreateButton({
   Name = "QuirkyCMD",
   Callback = function()
   loadstring(game:HttpGet("https://gist.github.com/someunknowndude/38cecea5be9d75cb743eac8b1eaf6758/raw"))()
   end,
})

